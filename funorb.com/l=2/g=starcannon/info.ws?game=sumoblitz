<script type="text/javascript">

  function appeaseGyojiBot() {
   document.getElementById("he_watches").style.display="none";
   document.getElementById("psa").style.display="block";
   setTimeout("scornGyojiBot()",2000);
  }

  function scornGyojiBot() {
   document.getElementById("psa").style.display="none";
   document.getElementById("he_watches").style.display="inline";
  }
  
  </script>
<div style="text-align:center;height:100px;"><img id="he_watches" src="http://www.funorb.com/l=2/img/images/game/sumoblitz/gyojibot.png" alt="GyojiBot vous aime." title="GyojiBot vous aime." onmousedown="if(event.preventDefault) event.preventDefault();appeaseGyojiBot();" /><p id="psa" style="display:none;line-height:100px;text-shadow: red 0px 0px 8px;color:red;font-size:18px;cursor:none;" onmousedown="return false;">Il ne dort jamais.</p></div>
</div>
