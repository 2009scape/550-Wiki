     function show() {
      document.getElementById("hideme").style.display="inline";
     }
     document.getElementById("hideme").style.display="none";
     setTimeout("show()", 10000);

     
     var redirect="";
     function showmanualredirect() {
      
      window.location=redirect;
      
      document.getElementById("manualredirectlink").href=redirect;
      document.getElementById("manualredirect").style.display="inline";
      document.getElementByID("manualRedir").style.display="block";
     }
     
     function manualredirect(redirect_in) {
      redirect=redirect_in;
      setTimeout("showmanualredirect()", 5000);
     }
