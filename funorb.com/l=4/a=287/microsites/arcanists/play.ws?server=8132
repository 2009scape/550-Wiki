//<![CDATA[
 document.domain='funorb.com';
 function cm() {
  return window.confirm("Wanneer je op deze koppeling klikt, verlaat je de pagina. Je verliest elk spel dat je nu aan het spelen bent.\nGa terug naar het hoofdmenu van het spel voordat je de pagina verlaat om je voortgang, hoogste scores en prestaties te behouden.\n\nKlik op OK als je de pagina wilt verlaten.");
  }
 function zap(username) {
   var ad1 = document.getElementById('ad1');
   var banner = document.getElementById('banner');
   var logo = document.getElementById('logo');
   // if the banner exist then zap it
   if (ad1)ad1.src=''; /* So ads are not served, consuming bandwidth and possibly producing unreliable stats */
   
   if(banner)banner.style.display='none';
   if(logo)logo.style.display='block';
  }
  function unzap() {
  
   var ad1 = document.getElementById('ad1');
   var banner = document.getElementById('banner');
   var logo = document.getElementById('logo');

   if (ad1)ad1.src='http://services.funorb.com/m=advert/l=4/a=287/showbanner.ws?size=739&amp;amp;country=-1'; /* Serve ads once more */
   
   if(banner)banner.style.display='block';
   if(logo)logo.style.display='none';
 }

//]]>
</script>
