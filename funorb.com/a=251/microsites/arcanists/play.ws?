//<![CDATA[
 document.domain='funorb.com';
 function cm() {
  return window.confirm("Clicking this link will leave the page. You will lose any game you are in the middle of.\nTo keep your progress, highscores and achievements, return to the game's main menu before leaving the page.\n\nClick OK if you would like to leave the page.");
  }
 function zap(username) {
   var ad1 = document.getElementById('ad1');
   var banner = document.getElementById('banner');
   var logo = document.getElementById('logo');
   // if the banner exist then zap it
   if (ad1)ad1.src=''; /* So ads are not served, consuming bandwidth and possibly producing unreliable stats */
   
   if(banner)banner.style.display='none';
   if(logo)logo.style.display='block';
  }
  function unzap() {
  
   var ad1 = document.getElementById('ad1');
   var banner = document.getElementById('banner');
   var logo = document.getElementById('logo');

   if (ad1)ad1.src='http://services.funorb.com/m=advert/a=251/showbanner.ws?size=739&amp;amp;country=-1'; /* Serve ads once more */
   
   if(banner)banner.style.display='block';
   if(logo)logo.style.display='none';
 }

//]]>
</script>
